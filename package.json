{
    "name": "sample-angularjs-migration",
    "version": "0.1.0",
    "description": "sample application to demonstrate angularjs migration",
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "src/**/*.interface.ts",
            "src/**/test-utilities/**/*.ts",
            "src/**/*.spec.ts"
        ],
        "extension": [
            ".ts"
        ],
        "reporter": [
            "text",
            "text-summary"
        ],
        "require": [
            "ts-node/register"
        ],
        "sourceMap": true,
        "instrument": true,
        "all": true
    },
    "scripts": {
        "lint-frontend": "tslint -c tslint.json 'src/frontend/**/*.ts'",
        "lint-backend": "tslint -c tslint.json 'src/backend/**/*.ts'",
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "clean-frontend": "rm -rf dist/frontend",
        "clean-backend": "rm -rf dist/backend",
        "clean": "rm -rf dist",
        "build-frontend": "npm run lint-frontend && tsc && cp -r config dist/frontend && copyfiles -e **/*.{ts,js,scss} -u 1 src/frontend/**/*.* dist && webpack --config webpack/development.config.js",
        "build-backend": "npm run lint-backend && tsc && cp -r config dist/backend",
        "build": "npm run build-backend && npm run build-frontend",
        "build-production": "npm run lint && tsc && cp -r config dist/frontend && cp -r config dist/backend && copyfiles -e **/*.{ts,js,scss} -u 1 src/frontend/**/*.* dist && webpack --config webpack/production.config.js",
        "rebuild-frontend": "npm run clean-frontend && npm run build-frontend",
        "rebuild-backend": "npm run clean-backend && npm run build-backend",
        "rebuild": "npm run clean && npm run build",
        "test-frontend": "karma start karma.conf.js",
        "test-backend-int": "npm run rebuild && set NODE_ENV=testing&& set DB_USER=testdb2019&& set DB_PASSWORD=testdb2019&& nyc mocha --opts \"mocha.opts\" \"src/**/integration-tests/**/*.spec.ts\"",
        "test-backend-unit": "npm run rebuild && nyc mocha --opts \"mocha.opts\" --exclude \"src/**/integration-tests/**/*.spec.ts\" \"src/**/*.spec.ts\"",
        "test-backend": "npm run rebuild && set NODE_ENV=testing&& set DB_USER=testdb2019&& set DB_PASSWORD=testdb2019&& nyc mocha --opts \"mocha.opts\" \"src/**/*.spec.ts\"",
        "start-collector": "cd dist/backend && node data-collector/app",
        "start-api": "cd dist/backend && node apis/v1/app",
        "start-frontend": "cd dist/frontend && node server",
        "start-backend": "set NODE_ENV=testing&& set DB_USER=testdb2019&& set DB_PASSWORD=testdb2019&& concurrently \"npm run start-collector\" \"npm run start-api\"",
        "start": "npm run rebuild && concurrently \"npm run start-collector\" \"npm run start-api\" \"npm run start-frontend\"",
        "dev-frontend": "concurrently \"npm run start-backend\" \"copyfiles -e **/*.{ts,js,scss} -u 1 src/frontend/**/*.* dist\" \"webpack-dev-server --config webpack/development.config.js\"",
        "dev-backend": "nodemon --verbose --watch src/backend -e ts --exec \"npm run rebuild && npm run start-backend\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/YiZhang-Paul/Angularjs-Angular-Migration.git"
    },
    "author": "Yi Zhang",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/YiZhang-Paul/Angularjs-Angular-Migration/issues"
    },
    "homepage": "https://github.com/YiZhang-Paul/Angularjs-Angular-Migration#readme",
    "dependencies": {
        "@angular/animations": "^7.2.13",
        "@angular/cdk": "^7.3.7",
        "@angular/common": "^7.2.13",
        "@angular/compiler": "^7.2.13",
        "@angular/core": "^7.2.13",
        "@angular/forms": "^7.2.13",
        "@angular/http": "^7.2.13",
        "@angular/material": "^7.3.7",
        "@angular/platform-browser": "^7.2.13",
        "@angular/platform-browser-dynamic": "^7.2.13",
        "@angular/router": "^7.2.13",
        "@uirouter/angular": "^3.0.0",
        "axios": "^0.18.0",
        "bcryptjs": "^2.4.3",
        "concurrently": "^4.1.0",
        "config": "^3.1.0",
        "cors": "^2.8.5",
        "express": "^4.16.4",
        "express-validator": "^5.3.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.5.2",
        "ng-click-outside": "^4.0.0",
        "ngx-toastr": "^10.0.2",
        "redis": "^2.8.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.4.0",
        "validator": "^10.11.0",
        "xss": "^1.0.6",
        "zone.js": "^0.8.29"
    },
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
        "@babel/plugin-proposal-throw-expressions": "^7.2.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/polyfill": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "@types/axios": "^0.14.0",
        "@types/chai": "^4.1.7",
        "@types/config": "0.0.34",
        "@types/cors": "^2.8.4",
        "@types/express": "^4.16.1",
        "@types/express-validator": "^3.0.0",
        "@types/mocha": "^5.2.6",
        "@types/mongodb": "^3.1.23",
        "@types/mongoose": "^5.3.26",
        "@types/node": "^10.14.4",
        "@types/redis": "^2.8.12",
        "@types/sinon": "^7.0.11",
        "@types/superagent": "^3.8.7",
        "@types/validator": "^10.11.0",
        "@types/webpack-env": "^1.13.9",
        "angular2-template-loader": "^0.6.2",
        "babel-loader": "^8.0.5",
        "chai": "^4.2.0",
        "chai-http": "^4.2.1",
        "copyfiles": "^2.1.0",
        "css-loader": "^2.1.1",
        "file-loader": "^3.0.1",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "istanbul-instrumenter-loader": "^3.0.1",
        "karma": "^4.1.0",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.2",
        "karma-coverage-istanbul-reporter": "^2.0.5",
        "karma-junit-reporter": "^1.2.0",
        "karma-mocha": "^1.3.0",
        "karma-mocha-reporter": "^2.2.5",
        "karma-ng-html2js-preprocessor": "^1.0.0",
        "karma-remap-coverage": "^0.1.5",
        "karma-sinon": "^1.0.5",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^3.0.5",
        "mocha": "^5.2.0",
        "ng-annotate": "^1.2.2",
        "ng-annotate-webpack-plugin": "^0.3.0",
        "node-sass": "^4.11.0",
        "nodemon": "^1.18.11",
        "nyc": "^14.0.0",
        "puppeteer": "^1.14.0",
        "raw-loader": "^2.0.0",
        "sass-loader": "^7.1.0",
        "sinon": "^7.3.2",
        "source-map-support": "^0.5.12",
        "style-loader": "^0.23.1",
        "to-string-loader": "^1.1.5",
        "ts-loader": "^5.3.3",
        "ts-node": "^7.0.1",
        "tslint": "^5.16.0",
        "tslint-microsoft-contrib": "^6.1.1",
        "typescript": "^3.4.4",
        "url-loader": "^1.1.2",
        "vsts-coverage-styles": "^1.0.8",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.0",
        "webpack-dev-server": "^3.3.1",
        "webpack-merge": "^4.2.1"
    }
}
